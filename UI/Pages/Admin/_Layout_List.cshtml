    <link rel="stylesheet" type="text/css" href="/Admin/_lib/globalCss/global.css" />
    <link rel="stylesheet" type="text/css" href="/Admin/_lib/font/css/fontello.css" />
    <link rel="stylesheet" type="text/css" href="/Admin/_lib/AdminUI2/UI.css" />
    <script type="text/javascript" src="/Admin/_lib/jquery.js"></script>
    <!--固定表格-->
    <link rel="stylesheet" type="text/css" href="/Admin/_lib/table-fixed/table-fixed.css" />
    <script type="text/javascript" src="/Admin/_lib/table-fixed/table-fixed.js"></script>
    <!--收缩表格-->
    <link rel="stylesheet" type="text/css" href="/Admin/_lib/table-resize/table-resize.css" />
    <script type="text/javascript" src="/Admin/_lib/table-resize/table-resize.js"></script>
    <!--提示框-->
    <link rel="stylesheet" type="text/css" href="/Admin/_lib/alert/alert.css" />
    <script type="text/javascript" src="/Admin/_lib/alert/alert.js"></script>
    <!--确认框-->
    <link rel="stylesheet" type="text/css" href="/Admin/_lib/confirm/confirm.css" />
