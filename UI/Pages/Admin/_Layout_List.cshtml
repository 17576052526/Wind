    <!--移动框-->
    <link rel="stylesheet" type="text/css" href="/Admin/_lib/box-move/box-move.css" />
    <script type="text/javascript" src="/Admin/_lib/box-move/box-move.js"></script>
    <script type="text/javascript">
        //批量删除
        function checkDelete(obj) {
            if (obj) {
                $('#form [name="checkID"]:checked').prop('checked', false);
                $(obj).parents('tr').find('[name=checkID]').prop('checked', true);
            }
            var check = $('#form [name="checkID"]:checked');
            if (check.length == 0) {
                $.alert('请选择删除的行');
            } else {
                $.confirm('确定删除？', function () {
                    $('#form').submit();
                }, "", function () { check.prop('checked', false); })
            }
        }
    </script>
    @RenderSection("head",false)
</head>
<body class="p-7.5">
    @RenderBody()
</body>
</html>