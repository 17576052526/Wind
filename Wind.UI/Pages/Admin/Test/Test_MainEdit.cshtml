@page
@model Wind.UI.Pages.Admin.Test.Test_MainEditModel

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" />
    <title></title>
    <link rel="stylesheet" type="text/css" href="/lib/global.css" />
    <link rel="stylesheet" type="text/css" href="/Admin/Css/listedit-merge.min.css" />
</head>
<body>
    <div class="padding10 height100">
        <div class="box height100">
            <div class="box-head flex">
                <div class="flex1">销售订单编辑</div>
                <div class="marginLR3g">
                    <input type="button" value="提交" class="btn-primary h-pre" />
                    <input type="button" value="重置" class="btn h-pre" />
                </div>
            </div>
            <div class="box-body">
                <div class="form-row">
                    <label class="form-item">
                        <span>姓名</span>
                        <input type="text" class="input-text" />
                    </label>
                    <label class="form-item">
                        <span>部门</span>
                        <input type="text" class="input-text" />
                    </label>
                    <label class="form-item">
                        <span>是否离职</span>
                        <input type="checkbox" />
                    </label>
                    <label class="form-item">
                        <span>公司</span>
                        <input type="text" class="input-text" />
                    </label>
                    <label class="form-item">
                        <span>性别</span>
                        <input type="text" class="input-text" />
                    </label>
                    <label class="form-item">
                        <span>年龄</span>
                        <input type="text" class="input-text" />
                    </label>
                    <label class="form-item">
                        <span>联系方式</span>
                        <input type="text" class="input-text" />
                    </label>
                </div>
            </div>
        </div>
    </div>
</body>
</html>